.login-border{
    border: 1px solid #ccc;
    padding: 0 30px 80px 30px;
    margin-top: 200px;
    border-radius: 10px;
}
.login-panel{
    display: flex;
    height: 240px;
    flex-direction: column;
}
.login-panel-header {
    text-align: center;
    color: #ccc;
    font-size: 28px;
    justify-content: center;
    align-items: center;
    margin-top: 60px;
}
img.login-panel-img{
    width: 60px;
    border-radius: 30px;
    margin: 0 auto;
}